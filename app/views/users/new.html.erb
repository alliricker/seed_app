
<h1> New User </h1>
<%= form_for @user do |f| %>
<p><%= f.label :name, "Name:" %> 
<%= f.text_field :name %> <br><br>
<%= f.label :email, "Email:" %> 
<%= f.text_field :email %> <br><br>
<%= f.label :password, "Password:" %> 
<%= f.password_field :password %> <br><br>
<%= f.label :company, "Company:" %> 
<%= f.text_field :company %> <br><br>
<%= f.label :title, "Job Title:" %> 
<%= f.text_field :title %> <br><br>
<%= f.submit %></p>
<% end %>
