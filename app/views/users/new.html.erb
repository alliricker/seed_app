
<h1> New User </h1>
<%= form_for @user do |f| %>
<p><%= f.label :name, "Name:" %> 
<%= f.text_field :name %> <br><br>
<%= f.label :email, "Email:" %> 
<%= f.text_field :email %> <br><br>
<%= f.label :password, "Password:" %> 
<%= f.password_field :password %> <br><br>
<%= f.label :company, "Company:" %> 
<%= f.text_field :company %> <br><br>
<%= f.label :title, "Job Title:" %> 
<%= f.text_field :title %> <br><br>
<%= f.submit %></p>
<% end %>
<a href="/auth/google_oauth2" target="_blank"><img src="https://hosting.photobucket.com/images/pp94/alliricker/btn_google_signin_light_normal_web_2x.png?width=180&height=180&fit=bounds" border="0" alt="btn_google_signin_light_normal_web_2x"/></a>